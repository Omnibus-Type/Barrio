feature aalt {
feature locl;
feature sups;
feature frac;
feature ordn;
feature calt;
feature salt;
feature ss01;
} aalt ;

feature locl {
script latn;
language TRK;
sub i by idotaccent;
language AZE;
sub i by idotaccent;
language ESP;
sub numbersign quotesingle by quotesingle.loclGUA;
sub numbersign Y by Y.loclGUA;
sub numbersign Yacute by Yacute.loclGUA;
sub numbersign Ycircumflex by Ycircumflex.loclGUA;
sub numbersign Ydieresis by Ydieresis.loclGUA;
sub numbersign Ygrave by Ygrave.loclGUA;
sub numbersign Y tilde by Ytilde.loclGUA;
sub numbersign tilde Y by Ytilde.loclGUA;
sub numbersign Y asciitilde by Ytilde.loclGUA;
sub numbersign asciitilde Y by Ytilde.loclGUA;
sub numbersign y by y.loclGUA;
sub numbersign yacute by yacute.loclGUA;
sub numbersign ycircumflex by ycircumflex.loclGUA;
sub numbersign ydieresis by ydieresis.loclGUA;
sub numbersign ygrave by ygrave.loclGUA;
sub numbersign y tilde by ytilde.loclGUA;
sub numbersign tilde y by ytilde.loclGUA;
sub numbersign y asciitilde by ytilde.loclGUA;
sub numbersign asciitilde y by ytilde.loclGUA;
sub numbersign dollar [G g] by guarani;
language CRT;
sub i by idotaccent;
language TAT;
sub i by idotaccent;
language CAT;
sub  l' periodcentered' l by ldot;
sub  L' periodcentered' L by Ldot;
language KAZ;
sub i by idotaccent;
language MOL;
sub Scedilla by Scommaaccent;
sub scedilla by scommaaccent;
sub Tcedilla by Tcommaaccent;
sub tcedilla by tcommaaccent;
language ROM;
sub Scedilla by Scommaaccent;
sub scedilla by scommaaccent;
sub Tcedilla by Tcommaaccent;
sub tcedilla by tcommaaccent;
language PLK;
sub Cacute by Cacute.loclPLK;
sub Nacute by Nacute.loclPLK;
sub Oacute by Oacute.loclPLK;
sub Sacute by Sacute.loclPLK;
sub Zacute by Zacute.loclPLK;
sub cacute by cacute.loclPLK;
sub nacute by nacute.loclPLK;
sub oacute by oacute.loclPLK;
sub sacute by sacute.loclPLK;
sub zacute by zacute.loclPLK;
sub acute by acute.loclPLK;
} locl ;

feature sups {
sub one by onesuperior;
sub two by twosuperior;
sub three by threesuperior;
} sups ;

feature frac {
sub one slash eight by oneeighth;
sub one slash four by onequarter;
sub one slash two by onehalf;
sub three slash four by threequarters;
sub one slash three by onethird;
sub two slash three by twothirds;
} frac ;

feature ordn {
sub [zero one two three four five six seven eight nine] [A a]' by ordfeminine;
sub [zero one two three four five six seven eight nine] [O o]' by ordmasculine;
} ordn ;

feature calt {
# consonantconsonant
sub @Con0 @Con0' by @Con1;
sub @Con1 @Con1' by @Con0;
 
# consonantotherconsonant
sub @Con0 [@Voc0 @Voc1 @Etc] @Con0' by @Con1;
sub @Con1 [@Voc0 @Voc1 @Etc] @Con1' by @Con0;
 
# consonantotherotherconsonant
sub @Con0 [@Voc0 @Voc1 @Etc] [@Voc0 @Voc1 @Etc] @Con0' by @Con1;
sub @Con1 [@Voc0 @Voc1 @Etc] [@Voc0 @Voc1 @Etc] @Con1' by @Con0;
 
# vowelvowel
sub @Voc0 @Voc0' by @Voc1;
sub @Voc1 @Voc1' by @Voc0;
 
# vowelothervowel
sub @Voc0 [@Con0 @Con1 @Etc] @Voc0' by @Voc1;
sub @Voc1 [@Con0 @Con1 @Etc] @Voc1' by @Voc0;
 
# vowelotherothervowel
sub @Voc0 [@Con0 @Con1 @Etc] [@Con0 @Con1 @Etc] @Voc0' by @Voc1;
sub @Voc1 [@Con0 @Con1 @Etc] [@Con0 @Con1 @Etc] @Voc1' by @Voc0;
 
# vowelotherotherothervowel
sub @Voc0 [@Con0 @Con1 @Etc] [@Con0 @Con1 @Etc] [@Con0 @Con1 @Etc] @Voc0' by @Voc1;
sub @Voc1 [@Con0 @Con1 @Etc] [@Con0 @Con1 @Etc] [@Con0 @Con1 @Etc] @Voc1' by @Voc0;

# numbers
sub zero zero' by zero.ss01;
sub one one' by one.ss01;
sub two two' by two.ss01;
sub three three' by three.ss01;
sub four four' by four.ss01;
sub five five' by five.ss01;
sub six six' by six.ss01;
sub seven seven' by seven.ss01;
sub eight eight' by eight.ss01;
sub nine nine' by nine.ss01;
} calt ;

feature liga {
sub f f by f_f;
sub f i by fi;
sub f l by fl;
} liga ;

feature salt {
sub zero by zero.ss01;
sub one by one.ss01;
sub two by two.ss01;
sub three by three.ss01;
sub four by four.ss01;
sub five by five.ss01;
sub six by six.ss01;
sub seven by seven.ss01;
sub eight by eight.ss01;
sub nine by nine.ss01;
} salt ;

feature ss01 {
sub zero by zero.ss01;
sub one by one.ss01;
sub two by two.ss01;
sub three by three.ss01;
sub four by four.ss01;
sub five by five.ss01;
sub six by six.ss01;
sub seven by seven.ss01;
sub eight by eight.ss01;
sub nine by nine.ss01;
} ss01 ;

feature cpsp {
pos @Uppercase <5 0 10 0>;
} cpsp ;

